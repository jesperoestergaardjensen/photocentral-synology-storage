{
    "name": "jesperoestergaardjensen/photocentral-synology-storage-client",
    "description": "Synology NAS storage for PhotoCentral, Client",
    "license": "gpl-3.0-only",
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.4",
        "jesperoestergaardjensen/photocentral-storage" : "dev-main",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "PhotoCentralSynologyStorageClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhotoCentralSynologyStorageClient\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "scripts": {
        "test": "vendor/bin/phpunit tests"
    }
}
